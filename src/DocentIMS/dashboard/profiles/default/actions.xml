<?xml version="1.0" encoding="utf-8"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
   <object name="site_actions" meta_type="CMF Action Category">
      <property name="title">Site actions</property>
      <object name="login" meta_type="CMF Action" i18n:domain="plone">
         <property name="title" i18n:translate="">Log in</property>
         <property name="description" i18n:translate=""></property>
         <property name="url_expr">python:f&quot;{plone_portal_state.navigation_root_url()}/login&quot;</property>
         <property name="link_target"></property>
         <property name="icon_expr">string:plone-login</property>
         <property name="available_expr">python:member is None</property>
         <property name="permissions">
            <element value="View"/>
         </property>
         <property name="visible">True</property>
         <property name="modal" type="text">{}</property>
      </object>
      <object name="logout" meta_type="CMF Action" i18n:domain="plone">
         <property name="title" i18n:translate="">Log out</property>
         <property name="description" i18n:translate=""></property>
         <property name="url_expr">python:f&quot;{plone_portal_state.navigation_root_url()}/logout&quot;</property>
         <property name="link_target"></property>
         <property name="icon_expr">string:plone-logout</property>
         <property name="available_expr">python:member is not None</property>
         <property name="permissions">
            <element value="View"/>
         </property>
         <property name="visible">True</property>
      </object>
   </object>
   <object name="user" meta_type="CMF Action Category">
      <object name="manager_controlpanel" meta_type="CMF Action" i18n:domain="plone">
         <property name="title" i18n:translate="">Manager Dashboard (Settings)</property>
         <property name="description" i18n:translate=""></property>
         <property name="url_expr">string:${portal_url}/@@medialog_controlpanel</property>
         <property name="link_target"></property>
         <property name="icon_expr">string:plone-controlpanel</property>
         <property name="available_expr"></property>
         <property name="permissions">
            <element value="Plone Site Setup: Overview"/>
         </property>
         <property name="visible">True</property>
      </object>

      <object name="preferences" meta_type="CMF Action" i18n:domain="plone">
         <property name="title" i18n:translate="">Preferences</property>
         <property name="description" i18n:translate=""></property>
         <property name="url_expr">python:f&quot;{plone_portal_state.navigation_root_url()}/@@personal-information&quot;</property>
         <property name="link_target"></property>
         <property name="icon_expr">string:plone-user</property>
         <property name="available_expr">python:member is not None</property>
         <property name="permissions">
            <element value="View"/>
         </property>
         <property name="visible">True</property>
      </object>
   </object>
</object>

